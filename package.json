    "@types/react-redux": "^7.0.1",
    "@types/react-router": "^4.0.32",
    "@types/react-router-config": "^1.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/uuid": "^3.4.4",
    "@types/warning": "^3.0.0",
    "connected-react-router": "^6.2.2",
    "coveralls": "*",
    "formik": "^1.3.2",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "jest-dom": "^2.1.0",
    "history": "^4.7.2",
    "lerna": "3.13.0",
    "lodash.template":"^4.0.0",
    "normalizr": "^3.3.0",
    "path-to-regexp": "^2.4.0",
    "qs": "^6.5.2",
    "razzle": "^3.0.0-alpha.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.4.0-beta.6",
    "react-router-config": "^4.4.0-beta.6",
    "react-router-dom": "^4.4.0-beta.6",
    "react-testing-library": "^5.8.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.0",
    "rimraf": "*",
    "rollup": "1.2.0",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-typescript2": "0.19.2",
    "tg-resources": "^3.1.1",
    "ts-jest": "23.10.5",
    "tslib": "1.9.3",
    "tslint": "5.12.1",
    "tslint-config-standard": "8.0.1",
    "tslint-react": "3.6.0",
    "typescript": "^3.3.3",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "lerna": "lerna",
    "create-package": "node scripts/bootstrapPackage.js",
    "bootstrap": "lerna bootstrap",
    "build": "lerna --ignore=example-razzle-app exec yarn build",
    "lint": "lerna --ignore=example-razzle-app exec yarn lint",
    "test": "jest",
    "testAll": "yarn test:coverage && yarn test:examples",
    "test:examples": "lerna --scope example-* exec -- yarn test --coverage",
    "test:coverage": "yarn test --coverage --maxWorkers 24",
    "precoveralls": "yarn test:coverage --no-cache",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "check-packages": "yarn clean && yarn build && yarn test && yarn lint",
    "bump-version": "lerna --ignore=examples/* version --exact",
    "prerun-publish": "yarn build",
    "run-publish": "lerna --ignore=examples/* publish --exact",
    "run-publish:loose": "lerna --ignore=examples/* publish",
    "run-publish:git": "lerna --ignore=examples/* publish from-git",
    "start-example": "cd examples/example-app && yarn dev",
    "clean": "lerna exec yarn clean && yarn clean-logs",
    "clean-incl-deps": "yarn clean && yarn clean-deps",
    "clean-deps": "rimraf packages/*/node_modules && rimraf yarn.lock && rimraf */**/yarn.lock && rimraf node_modules",
    "clean-logs": "rimraf *.log && rimraf */**/*.log"
  }
}
